{
  "name": "npm-task-contrib",
  "displayName": "Npm Task Contributor",
  "version": "4.4.0",
  "private": true,
  "categories": [],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/task-explorer",
    "directory": "packages/npm_task_contrib"
  },
  "publisher": "SAPSE",
  "main": "./dist/src/extension",
  "scripts": {
    "bundle": "webpack --mode production",
    "ci": "npm-run-all clean compile coverage bundle package",
    "ci:artifacts_only": "npm-run-all clean compile bundle package",
    "clean": "rimraf ./dist *.vsix",
    "compile": "tsc",
    "coverage": "nyc mocha",
    "package": "node ./scripts/package-vsix.js",
    "test": "mocha"
  },
  "contributes": {
    "taskDefinitions": [
      {
        "type": "npm",
        "required": [
          "label",
          "script"
        ],
        "properties": {
          "label": {
            "type": "string",
            "description": "Label"
          },
          "path": {
            "type": "string",
            "description": "Path to package.json"
          },
          "script": {
            "type": "string",
            "description": "Selected script"
          }
        }
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "dependencies": {
    "@sap_oss/task_contrib_types": "^4.3.1",
    "datauri": "4.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.9.0",
    "@types/vscode": "1.75.0",
    "proxyquire": "2.1.3",
    "vsce": "1.100.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "BASContributes": {
    "tasksExplorer": [
      {
        "type": "npm",
        "intent": "npm"
      }
    ]
  },
  "icon": "icon.png"
}
