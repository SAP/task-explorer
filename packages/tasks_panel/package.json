{
  "name": "vscode-tasks-explorer-tasks-panel",
  "displayName": "VSCode Tasks Panel",
  "version": "3.0.3",
  "publisher": "SAPSE",
  "keywords": [],
  "categories": [],
  "main": "./dist/src/extension",
  "files": [
    "dist",
    "src",
    "*.vsix"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tasks-explorer",
          "title": "TASK EXPLORER",
          "icon": "resources/task_explorer.svg",
          "when": "is-task-explorer-view-visible"
        }
      ]
    },
    "views": {
      "tasks-explorer": [
        {
          "id": "tasksPanel",
          "name": "",
          "when": "is-task-explorer-view-visible"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "tasks-explorer.editTask",
          "when": "false"
        },
        {
          "command": "tasks-explorer.deleteTask",
          "when": "false"
        },
        {
          "command": "tasks-explorer.executeTask",
          "when": "false"
        },
        {
          "command": "tasks-explorer.createTask",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "tasks-explorer.createTask",
          "when": "view == tasksPanel",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "tasks-explorer.editTask",
          "when": "viewItem == task"
        },
        {
          "command": "tasks-explorer.deleteTask",
          "when": "viewItem == task"
        },
        {
          "command": "tasks-explorer.executeTask",
          "when": "viewItem == task",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "tasks-explorer.executeTask",
        "title": "Run",
        "icon": {
          "light": "resources/play_light.svg",
          "dark": "resources/play_dark.svg"
        }
      },
      {
        "command": "tasks-explorer.createTask",
        "title": "Create Task",
        "icon": {
          "light": "resources/create_task_light.svg",
          "dark": "resources/create_task_dark.svg"
        }
      },
      {
        "command": "tasks-explorer.editTask",
        "title": "Edit"
      },
      {
        "command": "tasks-explorer.deleteTask",
        "title": "Delete"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VSCode Tasks Panel",
      "properties": {
        "VSCode Tasks Panel.loggingLevel": {
          "type": "string",
          "enum": [
            "off",
            "fatal",
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "default": "error",
          "description": "The verbosity of logging according to the following order: trace > debug > info > warn > error > fatal > off.",
          "scope": "resource"
        },
        "VSCode Tasks Panel.sourceLocationTracking": {
          "type": "boolean",
          "default": false,
          "description": "If chosen, the location of the source code is added to log entries. Warning â€“ this action may slow your extension. We recommend you use it only for debugging.",
          "scope": "resource"
        }
      }
    }
  },
  "repository": "https://github.com/SAP/task-explorer",
  "engines": {
    "vscode": "^1.50.0"
  },
  "scripts": {
    "ci": "npm-run-all clean compile coverage bundle frontend:copy package",
    "ci:artifacts_only": "npm-run-all clean compile bundle frontend:copy package",
    "clean": "rimraf ./dist *.vsix",
    "compile": "tsc",
    "coverage": "nyc mocha",
    "test": "mocha",
    "bundle": "webpack --mode production",
    "package": "node ./scripts/package-vsix.js",
    "frontend:copy": "webpack --mode none --config ./copy.frontend.webpack.config.js",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@sap-devx/webview-rpc": "0.2.3",
    "@sap/swa-for-sapbas-vsx": "1.2.14",
    "@vscode-logging/logger": "1.2.2",
    "@vscode-tasks-explorer/task_contrib_types": "^3.0.3",
    "datauri": "2.0.0",
    "escape-string-regexp": "4.0.0",
    "fs-extra": "8.1.0",
    "lodash": "4.17.21",
    "strip-ansi": "6.0.0",
    "ws": "7.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.150",
    "@types/sinon": "^9.0.4",
    "@types/vscode": "1.50.0",
    "@types/ws": "^7.4.0",
    "@vscode-tasks-explorer/vue_frontend_rpc": "^3.0.3",
    "proxyquire": "2.1.3",
    "sinon": "^9.0.3",
    "vsce": "1.79.5",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  }
}
